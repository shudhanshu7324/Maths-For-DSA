package MathsForDSA;

public class FindNonDuplicate {
    public static void main(String[] args) {
        int[] array = {2,4,6,7,4,6,2};
        findNonDuplicate(array);
    }
    public static int findNonDuplicate(int[] array){
        int unique = 0;
        for (int n:array){
            unique^= n;
        }
        return unique;
    }
}
